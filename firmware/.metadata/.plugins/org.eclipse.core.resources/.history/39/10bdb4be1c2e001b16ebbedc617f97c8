#include "test_framework.hh"

void TestCreateAS5048A() {
	TPRINT("TestCreateAS5048A\r\n");
	SPI_HandleTypeDef * dummy_spi = NULL;
	AS5048A enc = AS5048A(dummy_spi);

	if (enc.get_angle() != 0) {
		FAILPRINT("Initialized with wrong value");
	}

}


