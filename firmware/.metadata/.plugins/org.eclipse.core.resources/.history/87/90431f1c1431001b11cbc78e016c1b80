#ifndef MOTOR_INC_HALF_BRIDGE_HH_
#define MOTOR_INC_HALF_BRIDGE_HH_

class HalfBridge {
private:
	float target_current_{0};

public:
	virtual void Init();
	virtual void Update();
	void set_target_current(float new_target_current);
	float get_target_current();

};



#endif /* MOTOR_INC_HALF_BRIDGE_HH_ */
